{
  "kind": "singleType",
  "collectionName": "case_studies",
  "info": {
    "singularName": "case-study",
    "pluralName": "case-studies",
    "displayName": "Case Study"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "slug": {
      "type": "string"
    },
    "title": {
      "type": "string"
    },
    "short_description": {
      "type": "string"
    },
    "tags": {
      "type": "enumeration",
      "enum": [
        "gained efficiency,",
        "quality success rate"
      ]
    },
    "cover_Image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "problem": {
      "type": "string"
    },
    "solution": {
      "type": "text"
    },
    "outcome": {
      "type": "component",
      "component": "case-study.outcomes",
      "repeatable": true
    },
    "results": {
      "type": "component",
      "component": "case-study.results",
      "repeatable": true
    },
    "project_info": {
      "type": "component",
      "component": "case-study.project-info",
      "repeatable": false
    },
    "overview_cards": {
      "type": "component",
      "component": "case-study.overview-cards",
      "repeatable": true
    },
    "project_description": {
      "type": "component",
      "component": "case-study.project-description",
      "repeatable": false
    },
    "website_url": {
      "type": "string"
    },
    "website_label": {
      "type": "string"
    },
    "galleries": {
      "type": "component",
      "component": "case-study.gallery",
      "repeatable": true
    }
  }
}
